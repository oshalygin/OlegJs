{
  "name": "olegjs",
  "version": "1.0.4",
  "description": "OlegJs Web Application",
  "engines": {
    "npm": ">=3"
  },
  "scripts": {
    "preinstall": "node utilities/nodeVersionCheck.js",
    "setup": "node utilities/setup/setupMessage.js && npm install && node utilities/setup/setup.js",
    "start-message": "babel-node utilities/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel test:watch open:server lint",
    "open:server": "babel-watch server/application.dev.js",
    "open:dist": "babel-node server/application.production.js",
    "lint": "eslint webpack.config.* \"client/**\" \"server/**\" utilities/** --color ",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist && npm run lint && npm run test",
    "build": "babel-node utilities/build.js",
    "test": "mocha utilities/testSetup.js \"client/**/*.spec.js\" --reporter spec",
    "test:cover": "babel-node node_modules/isparta/bin/isparta cover --root client --report lcov _mocha -- --require ./utilities/testSetup.js \"client/**/*.spec.js\" --reporter spec",
    "test:cover:travis": "babel-node node_modules/isparta/bin/isparta cover --root client --report lcovonly _mocha -- --require ./utilities/testSetup.js \"client/**/*.spec.js\" && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "npm run test -- --watch",
    "open:cover": "npm run test:cover && open coverage/index.html"
  },
  "author": "oshalygin@gmail.com",
  "private": true,
  "license": "MIT",
  "dependencies": {
    "axios": "0.15.2",
    "body-parser": "1.15.2",
    "bootstrap": "^3.3.7",
    "express": "4.14.0",
    "express-stormpath": "3.1.6",
    "font-awesome": "^4.7.0",
    "jquery": "2.2.4",
    "jquery.backstretch": "2.1.13",
    "jquery.simple-text-rotator": "0.0.1",
    "material-ui": "0.16.2",
    "morgan": "1.7.0",
    "object-assign": "4.1.0",
    "react": "15.4.1",
    "react-css-modules": "4.0.3",
    "react-dom": "15.4.1",
    "react-redux": "4.4.5",
    "react-router": "2.7.0",
    "react-router-redux": "4.0.5",
    "react-stormpath": "1.3.1",
    "react-tap-event-plugin": "1.0.0",
    "redux": "3.5.2",
    "redux-thunk": "2.1.0",
    "smoothscroll": "^0.2.2",
    "winston": "2.2.0",
    "wowjs": "^1.1.3"
  },
  "devDependencies": {
    "autoprefixer": "6.4.0",
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "babel-eslint": "7.0.0",
    "babel-loader": "6.2.5",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-transform-object-rest-spread": "6.16.0",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-remove-prop-types": "0.2.9",
    "babel-preset-latest": "6.14.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.13.0",
    "babel-register": "6.14.0",
    "babel-watch": "2.0.3",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "coveralls": "2.11.12",
    "cross-env": "2.0.1",
    "css-loader": "0.24.0",
    "enzyme": "2.4.1",
    "eslint": "3.4.0",
    "eslint-plugin-import": "1.14.0",
    "eslint-plugin-jsx-a11y": "2.2.0",
    "eslint-plugin-react": "6.2.0",
    "eslint-watch": "2.1.14",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.22.0",
    "isparta": "4.0.0",
    "istanbul": "0.4.4",
    "jsdom": "9.8.0",
    "jsdom-global": "2.1.0",
    "json-loader": "0.5.4",
    "mocha": "3.0.2",
